{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/slack:v2.*.*",
        "alias": "slack",
        "title": "Slack",
        "description": "Authorize this connector if you want to use the Slack API."
      }, {
        "name": "transposit/zoom:v1.*.*",
        "alias": "zoom"
      }],
      "operations": [{
        "id": "webhook",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{   \"query_parameters\": {     \"api_key\": \"XXX\"   },   \"http_method\": \"POST\",   \"parsed_body\": {     \"channel_name\": \"XXX\",     \"user_id\": \"UXXX\",     \"user_name\": \"iggy\",     \"trigger_id\": \"XXX\",     \"team_domain\": \"my_domain\",     \"team_id\": \"TXXX\",     \"text\": \"\",     \"channel_id\": \"DXXX\",     \"command\": \"/test\",     \"token\": \"XXX\",     \"response_url\": \"https://hooks.slack.com/commands/TXXX/XXX/XXX\"   },   \"body\": \"token=XXX&team_id=TXXX&team_domain=my_domain&channel_id=DXXX&channel_name=XXX&user_id=UXXX&user_name=iggy&command=%2Ftest&text=&response_url=https%3A%2F%2Fhooks.slack.com%2Fcommands%2FTXXX%2FXXX%2FXXX&trigger_id=XXX\" }"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js",
          "lang": "js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "get_slack_message",
        "function": {
          "parameters": [{
            "name": "user",
            "type": "STRING"
          }],
          "js": "get_slack_message.js",
          "lang": "js"
        }
      }, {
        "id": "recordings_list",
        "tql": {
          "parameters": [{
            "name": "start_date",
            "description": "start date, for example '2019-10-10'",
            "type": "STRING",
            "required": true
          }, {
            "name": "end_date",
            "description": "end date, for example '2019-10-10'",
            "type": "STRING"
          }],
          "tql": "SELECT rl.share_url, rl.start_time, rl.topic, rl.id FROM zoom.recordings_list as rl \nJOIN zoom.users as u\nON rl.userId = u.id\nWHERE `from`=@start_date\nAND to=@end_date\nAND trash=false\n"
        }
      }, {
        "id": "post_chat_message",
        "tql": {
          "parameters": [{
            "name": "channel",
            "description": "Channel to put video archive in",
            "type": "STRING",
            "required": true
          }, {
            "name": "text",
            "description": "Text to send",
            "type": "STRING",
            "required": true
          }],
          "tql": "SELECT * FROM slack.post_chat_message\n  WHERE $body=(SELECT {\n    'channel' : @channel,\n    'mrkdwn' : false,\n    'as_user' : false,\n    'text' : @text,\n  })\n"
        }
      }, {
        "id": "post_recording_to_slack",
        "function": {
          "js": "post_recording_to_slack.js",
          "lang": "js"
        }
      }],
      "dataConnections": [{
        "ref": "slack_identify.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "identity.basic"
            },
            "executionLimits": {
            }
          }
        },
        "title": "Slack",
        "description": "Used to identify you on Slack"
      }, {
        "ref": "slack_bot.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "chat:write:bot"
            },
            "executionLimits": {
            }
          }
        }
      }, {
        "ref": "slack.raw"
      }],
      "javascriptAPIVersion": "3",
      "source": "src",
      "userSettingsDisplay": "GENERATED"
    }
  }
}